  "homepage": "/",
  "dependencies": {
    "@11ty/eleventy": "*",
    "@11ty/eleventy-img": "^0.9.0",
    "cross-env": "^7.0.3",
    "npm-run-all": "^4.1.5",
    "sass": "^1.32.12",
    "text-balancer": "^1.0.5",
    "webpack": "^5.36.2",
    "webpack-cli": "^4.6.0"
  }
